		End if		
		
		//[生年月日]：日付が間違っています
		If ImageFound(3,"RegistNew_Birthday_mistake_err_msg") then 
			//検出
			Log "[生年月日]日付チェック：OK"
		Else
			//未検出
			LogError "[生年月日]日付チェック：NG"
