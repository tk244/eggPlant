			Log "[生年月日]日付チェック：OK"
		Else
			//未検出
			LogError "[生年月日]日付チェック：NG"
		End if
		
	End if
	
	If no = 5 then
		
		//[パスワード]が一致しません
		If ImageFound(3,"RegistNew_password_Disagreement_err_msg") then 
			//検出
